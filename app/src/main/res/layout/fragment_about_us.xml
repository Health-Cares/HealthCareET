<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"

>
<FrameLayout
        android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".AboutUsFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="Health care"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView23"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                android:layout_marginTop="52dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <ImageView
                android:layout_width="146dp"
                android:layout_height="58dp" app:srcCompat="@drawable/index"
                app:layout_constraintTop_toBottomOf="@+id/textView23"
                android:layout_marginTop="28dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:id="@+id/imageView2"/>

        <TextView
                android:text="Version: 1.0.0-2019"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/imageView2"
        />
        <TextView
                android:text="CopyrightÂ© 2019 Health Care"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView2"
                app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginTop="16dp"
                tools:ignore="MissingConstraints" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"/>
        <TextView
                android:text="This android application is mainly used for health related issues."
                android:layout_width="320dp"
                android:layout_height="37dp"
                android:id="@+id/textView5" android:layout_marginTop="24dp"
                app:layout_constraintTop_toBottomOf="@+id/textView2" tools:ignore="MissingConstraints"
                android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="WWW.HealthCare@gmail.com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView6" android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/textView5" tools:ignore="MissingConstraints"
                android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
</layout>