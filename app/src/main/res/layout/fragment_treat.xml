<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"

>
<FrameLayout
        android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.example.healthcareet.TreatFragment">


    <androidx.core.widget.NestedScrollView

            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="499dp"
                android:orientation="vertical">
            <ImageButton
                    android:layout_width="400dp"
                    android:layout_height="200dp"
                    app:srcCompat="@drawable/tootht"
                    android:layout_marginBottom="20dp"
                    android:id="@+id/toothache_image_button" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintHorizontal_bias="0.54"/>
            <ImageButton
                    android:layout_width="400dp"
                    android:layout_height="200dp" app:srcCompat="@drawable/headt"
                    android:layout_marginBottom="20dp"
                    android:id="@+id/headache_image_button" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/toothache_image_button" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"/>
            <ImageButton
                    android:layout_width="400dp"
                    android:layout_height="200dp" app:srcCompat="@drawable/musclet"
                    android:id="@+id/muscle_image_button"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/headache_image_button" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.496" android:visibility="visible"/>

            <ImageButton
                    android:layout_width="400dp"
                    android:layout_height="200dp" app:srcCompat="@drawable/commont"
                    android:id="@+id/common_image_button"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/headache_image_button" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.496" android:visibility="visible"/>
            <ImageButton
                    android:layout_width="400dp"
                    android:layout_height="200dp" app:srcCompat="@drawable/asthmat"
                    android:id="@+id/asthma_image_button"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/headache_image_button" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.496" android:visibility="visible"/>
            <ImageButton
                    android:layout_width="400dp"
                    android:layout_height="200dp" app:srcCompat="@drawable/heartt"
                    android:id="@+id/heart_image_button"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/headache_image_button" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.496" android:visibility="visible"/>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</FrameLayout>
</layout>